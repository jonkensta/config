[core]
	editor = nvim
	autocrlf = false

[push]
    default = simple
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[diff]
    tool = vimdiff
[difftool]
	prompt = false
[alias]
	conflicted = !nvim +Conflicted
